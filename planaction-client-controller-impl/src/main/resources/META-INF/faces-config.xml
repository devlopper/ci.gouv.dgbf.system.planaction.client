<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<!-- action plan -->
		
		<navigation-case>
			<from-outcome>actionPlanListView</from-outcome>
			<to-view-id>/private/actionplan/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actionPlanEditView</from-outcome>
			<to-view-id>/private/actionplan/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actionPlanReadView</from-outcome>
			<to-view-id>/private/actionplan/read.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- action plan and activity -->
		
		<navigation-case>
			<from-outcome>actionPlanActivityCreateView</from-outcome>
			<to-view-id>/private/actionplan/activity/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actionPlanActivityEditView</from-outcome>
			<to-view-id>/private/actionplan/activity/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- imputation -->
		
		<navigation-case>
			<from-outcome>imputationListView</from-outcome>
			<to-view-id>/private/imputation/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>imputationCreateView</from-outcome>
			<to-view-id>/private/imputation/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>imputationEditView</from-outcome>
			<to-view-id>/private/imputation/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>imputationReadView</from-outcome>
			<to-view-id>/private/imputation/read.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		
		
		<!-- 
		
		<navigation-case>
			<from-outcome>actionPlanFinancialProgramView</from-outcome>
			<to-view-id>/private/actionplan/financialprogram.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actionPlanEntryAuthorizationPlanView</from-outcome>
			<to-view-id>/private/actionplan/entryauthorizationplan.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		 -->
		
	</navigation-rule>

</faces-config>
